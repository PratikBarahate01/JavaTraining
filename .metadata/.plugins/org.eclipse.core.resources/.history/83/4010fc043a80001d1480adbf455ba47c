package Employee;

import java.util.Collections;
import java.util.List;
import java.util.stream.Collectors;


public class Main {
	public static void main(String[] args) {
		
		System.out.println(getSortedList());
	}
	
	public static List<Model> getSortedList(){
		EmployeeDao dao = new EmployeeDao();
		List<Model> modelList = dao.getModelList();
		
		
		modelList.stream().collect(Collectors.groupingBy(Model::getLocation, Collectors.counting()));
		
		
//		Collections.sort(modelList,(e1, e2)-> e1.getName().compareTo(e2.getName()));
//		return modelList;
		
		
		//List<Model> sortedList = modelList.stream().sorted((e1, e2)->e1.).collect(Collectors.toList());
	}
}
