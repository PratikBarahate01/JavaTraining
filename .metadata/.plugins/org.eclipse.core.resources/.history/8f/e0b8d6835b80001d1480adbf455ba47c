package mapVsflatmap;

public class Employee {
	private String name;
	private int panno;
	public Employee(String name, String string) {
		super();
		this.name = name;
		this.panno = string;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPanno() {
		return panno;
	}
	public void setPanno(int panno) {
		this.panno = panno;
	}
	@Override
	public String toString() {
		return "EmployeeDao [name=" + name + ", panno=" + panno + "]";
	}
	
	
}
