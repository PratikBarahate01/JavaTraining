package thread;

import java.util.Iterator;

public class ThreadLambdaExample extends Thread{
	public static void main(String[] args) throws InterruptedException {
		for (int i = 0; i < 5; i++) {
			System.out.println("Main thread...");
			Thread.sleep(1000);
		}
		
		Thread t = new Thread(()->{
			for (int i = 0; i < 5; i++) {
				System.out.println("Child Thread...");
				Thread.sleep(1000)
			}
		});
		
		t.start();
	}
	
	
}
